@model IEnumerable<AspNetModule1.Models.Comment>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Data)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.CommentaireId }) |
            @Html.ActionLink("Details", "Details", new { id = item.CommentaireId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.CommentaireId })
        </td>
    </tr>
}
